{
    "endOfLine": "lf",
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "printWidth": 120,
    "phpVersion": "8.3",
    "plugins": ["@prettier/plugin-php"],
    "trailingComma": "es5",
    "overrides": [
        {
            "files": ["*.json", "*.md"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
